<div class="row col-md-9 col-lg-9">
    <div class="col-md-9 col-lg-9">
        <h2>SignUp</h2>

        <form class="form signup-form" autocomplete="off">
            <div class="form-group">
                <label for="name-signup">Username:</label>
                <input class="form-input" type="text" id="username-signup" autocomplete="new-username" />
            </div>
            <div class="form-group">
                <label for="email">Email:</label>
                <input class="form-input" type="text" id="email-signup" autocomplete="new-email"/>
            </div>
            <div class="form-group">
                <label for="password">Password:</label>
                <input class="form-input" type="password" id="password-signup" autocomplete="new-password"/>
            </div>
            <div class="form-group">
                <button class="btn btn-primary" type="submit">Signup</button>
            </div>
        </form>
    </div>
</div>

<script src="/public/js/signup.js"></script>
